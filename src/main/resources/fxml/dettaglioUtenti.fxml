<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>
<VBox fx:controller="com.nsi.fxmlController.DettaglioUtentiController" prefHeight="400.0" prefWidth="600.0"
      xmlns="http://javafx.com/javafx/8.0.76-ea" xmlns:fx="http://javafx.com/fxml/1" fx:id="fxDettaglioUtente">
    <children>

        <Label prefHeight="25.0" prefWidth="62.0" text="Nome">
            <padding>
                <Insets left="5.0"/>
            </padding>
        </Label>
        <TextField fx:id="fxNome" maxWidth="-Infinity" prefHeight="25.0" prefWidth="200.0">
            <padding>
                <Insets left="5.0"/>
            </padding>
        </TextField>
        <Label prefHeight="41.0" prefWidth="81.0" text="Cognome">
            <padding>
                <Insets left="5.0"/>
            </padding>
        </Label>
        <TextField fx:id="fxCognome" layoutX="10.0" layoutY="60.0" maxWidth="-Infinity" prefHeight="25.0"
                   prefWidth="200.0"/>
        <Label prefHeight="43.0" prefWidth="80.0" text="Matricola">
            <padding>
                <Insets left="5.0"/>
            </padding>
        </Label>
        <TextField fx:id="fxMatricola" layoutX="10.0" layoutY="169.0" maxWidth="-Infinity" prefHeight="25.0"
                   prefWidth="200.0">
            <padding>
                <Insets left="5.0"/>
            </padding>
        </TextField>

        <Label prefHeight="43.0" prefWidth="80.0" text="Ufficio">
            <padding>
                <Insets left="5.0"/>
            </padding>
        </Label>
        <TextField fx:id="fxUfficio" layoutX="10.0" layoutY="169.0" maxWidth="-Infinity" prefHeight="25.0"
                   prefWidth="200.0">
            <padding>
                <Insets left="5.0"/>
            </padding>
        </TextField>
        <Button fx:id="fxSaveButton" mnemonicParsing="false" text="Save"/>
    </children>
</VBox>
